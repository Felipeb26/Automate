            </BorderPane.margin>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </HBox>
    </top>
    <left>

    </left>
    <right>

    </right>
    <bottom>

    </bottom>
    <center>

    </center>
   <left>
      <VBox prefHeight="650.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
                <Button fx:id="btnMain" mnemonicParsing="false" prefHeight="60.0" prefWidth="174.0" text="MAIN">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="abouts" mnemonicParsing="false" prefHeight="62.0" prefWidth="180.0" text="ABOUT">

                        <Font size="14.0" />
                    </font>
                    <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="bntClose" mnemonicParsing="false" onMouseClicked="#closeStage" prefHeight="16.0" prefWidth="30.0" text="X" wrapText="true" HBox.hgrow="ALWAYS">

                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefHeight="62.0" prefWidth="181.0" text="CONFIG">
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Button>
            <Button disable="true" mnemonicParsing="false" prefHeight="62.0" prefWidth="180.0">
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button disable="true" mnemonicParsing="false" prefHeight="62.0" prefWidth="182.0">
               <VBox.margin>
                  <Insets bottom="20.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button disable="true" mnemonicParsing="false" prefHeight="62.0" prefWidth="183.0">
